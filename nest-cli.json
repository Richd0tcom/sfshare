{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "**/*.csv","watchAssets": true },
      { "include": "**/*.conf", "watchAssets": true }
    ]
  }
}
